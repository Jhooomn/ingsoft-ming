<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.ingsoft.dao.LibraryDao">

    <select id="UserGames" resultType="Integer">
        SELECT
               id_game as idGame
        FROM library
        WHERE
            id_user = #{idUser}
    </select>
</mapper>
